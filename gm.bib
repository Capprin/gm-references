	Eprint = {http://www.pnas.org/content/107/51/21979.full.pdf+html},
	Journal = {Proceedings of the National Academy of Sciences},
	Number = {51},
	Pages = {21979-21984},
	Title = {Soap-film M{\"o}bius strip changes topology with a twist singularity},
	Url = {http://www.pnas.org/content/107/51/21979.abstract},
